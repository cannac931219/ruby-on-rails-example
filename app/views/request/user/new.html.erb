<div style="color: #000;position: relative;top: 20px;left: 270px;">
	<div>
		<h3>创建用户</h3>
		<div>
			<%= form_for @user, :html => {:multipart => true} do |f| %>
			  <%= f.error_messages %>
			  <%= f.hidden_field :gallery_id %>
			  <p>
			    <%= f.label :name %><br />
			    <%= f.text_field :name %>
			  </p>
			  <p>
			    <%= f.file_field :image %>
			  </p>
			  <p>
			    <%= f.label :remote_image_url, "or image URL" %><br />
			    <%= f.text_field :remote_image_url %>
			  </p>
			  <p><%= f.submit %></p>
			<% end %>
		</div>
	</div>
</div>
